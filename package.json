  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "debug": "npm-run-all --parallel frontend debug-server",
    "debug-server": "nodemon --inspect-brk=0.0.0.0:9229 ./index.js",
    "frontend": "cd ./frontend && yarn install && yarn run start",

