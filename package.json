    "latency-server": "node ./latency.js"
  },
  "dependencies": {
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "http-server": "^0.10.0",
    "lowdb": "^1.0.0",
    "mediainfo-parser": "^1.1.5",
    "node-storage": "^0.0.7",
    "npm-run-all": "^4.1.2",
    "q": "^1.5.1",
    "ws": "^4.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.14.7"
  }
}
